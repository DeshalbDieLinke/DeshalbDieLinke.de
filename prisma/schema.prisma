generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Content {
  id        Int     @id @default(autoincrement())
  title     String
  content   String?
  published Boolean @default(false)
  official  Boolean @default(false) 
  authorId  String?
  alt       String?
  type      String?
}
